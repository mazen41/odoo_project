<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="engineering_project_stage_new" model="project.task.type">
        <field name="name">New (جديد)</field>
        <field name="sequence">1</field>
    </record>

    <record id="engineering_project_stage_doc_collection" model="project.task.type">
        <field name="name">Document Collection (استلام الاوراق)</field>
        <field name="sequence">5</field>
    </record>

    <record id="engineering_project_stage_contract_creation" model="project.task.type">
        <field name="name">Contracting &amp; Document Collection (عمل العقد والدفعة الاولى)</field>
        <field name="sequence">10</field>
    </record>

    <record id="engineering_project_stage_arch_design" model="project.task.type">
        <field name="name">Architectural Design (التصميم المعماري)</field>
        <field name="sequence">20</field>
    </record>

    <record id="engineering_project_stage_struct_design" model="project.task.type">
        <field name="name">Structural Design (سيستم الاعمدة الانشائية)</field>
        <field name="sequence">30</field>
    </record>

    <record id="engineering_project_stage_municipality_sub" model="project.task.type">
        <field name="name">Municipality Submission (ادخال المعاملة الى البلدية)</field>
        <field name="sequence">40</field>
    </record>

    <record id="engineering_project_stage_supervision_pledge" model="project.task.type">
        <field name="name">Supervision Pledge (تعهيد اشراف)</field>
        <field name="sequence">50</field>
    </record>

    <record id="engineering_project_stage_on_site_supervision" model="project.task.type">
        <field name="name">On-Site Supervision (الاشراف على القسيمة)</field>
        <field name="sequence">60</field>
    </record>

    <record id="engineering_project_stage_completed" model="project.task.type">
        <field name="name">Completed (إنهاء إشراف)</field>
        <field name="sequence">100</field>
        <field name="fold" eval="True"/>
    </record>

    <record id="engineering_project_stage_archived" model="project.task.type">
        <field name="name">Archived (مؤرشف)</field>
        <field name="sequence">110</field>
        <field name="fold" eval="True"/>
    </record>

</odoo>
